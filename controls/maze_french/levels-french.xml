  </Panel>
  <Panel>
    <Icon>Disc</Icon>
    <Type>Home</Type>
    <Page>
      <Name>Labyrinthe</Name>
      <Row>
        <Name>Score</Name>
        <Widget>
          <WidgetId>score</WidgetId>
          <Name> </Name>
          <Type>Text</Type>
          <Options>size=2;fontSize=normal</Options>
        </Widget>
        <Widget>
          <WidgetId>restart</WidgetId>
          <Name>Redémarrer</Name>
          <Type>Button</Type>
          <Options>size=2</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Instructions</Name>
        <Widget>
          <WidgetId>instructions</WidgetId>
          <Name>Bienvenue dans le labyrinthe: partez à la recherche du trésor. Choisissez une direction...</Name>
          <Type>Text</Type>
          <Options>size=4;align=left;fontSize=normal</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Direction</Name>
        <Widget>
          <WidgetId>directions</WidgetId>
          <Name>aide</Name>
          <Type>DirectionalPad</Type>
          <Options>size=4</Options>
        </Widget>
      </Row>
      <Options>hideRowNames=0</Options>
    </Page>
    <Page>
      <Name>Règles</Name>
      <Row>
        <Name>Instructions</Name>
        <Widget>
          <WidgetId>rules</WidgetId>
          <Name>Parcourez le labyrinthe à la recherche du trésor. Appuyez sur 'Aide' pour faire apparaître la carte. Attention, il vous en coûtera 500 points à chaque fois.</Name>
          <Type>Text</Type>
          <Options>size=4;fontSize=small</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Symboles</Name>
        <Widget>
          <WidgetId>symbol_location</WidgetId>
          <Name>'o' : vous êtes ici</Name>
          <Type>Text</Type>
          <Options>size=2;fontSize=small</Options>
        </Widget>
        <Widget>
          <WidgetId>symbol_treasure</WidgetId>
          <Name>'?' : le trésor (+5.000)</Name>
          <Type>Text</Type>
          <Options>size=2;fontSize=small</Options>
        </Widget>
      </Row>
      <Row>
        <Name> </Name>
        <Widget>
          <WidgetId>symbol_nothing</WidgetId>
          <Name>'_' : rien ici (+50)</Name>
          <Type>Text</Type>
          <Options>size=2;fontSize=small</Options>
        </Widget>
        <Widget>
          <WidgetId>symbol_border</WidgetId>
          <Name>'|' : une bordure (-200)</Name>
          <Type>Text</Type>
          <Options>size=2;fontSize=small</Options>
        </Widget>
      </Row>
      <Row>
        <Name> </Name>
        <Widget>
          <WidgetId>symbol_wall</WidgetId>
          <Name>'X' : un mur (-100)</Name>
          <Type>Text</Type>
          <Options>size=2;fontSize=small</Options>
        </Widget>
        <Widget>
          <WidgetId>symbol_cat</WidgetId>
          <Name>'C' : un chat (+200)</Name>
          <Type>Text</Type>
          <Options>size=2;fontSize=small</Options>
        </Widget>
      </Row>
      <Row>
        <Name> </Name>
        <Widget>
          <WidgetId>symbol_dog</WidgetId>
          <Name>'D' : un chien (-200)</Name>
          <Type>Text</Type>
          <Options>size=2;fontSize=small</Options>
        </Widget>
        <Widget>
          <WidgetId>symbol_monster</WidgetId>
          <Name>'M': un monstre (-500)</Name>
          <Type>Text</Type>
          <Options>size=2;fontSize=small</Options>
        </Widget>
      </Row>
      <Options>hideRowNames=0</Options>
    </Page>
    <Page>
      <Name>Options</Name>
      <Row>
        <Name>Niveau</Name>
        <Widget>
          <WidgetId>difficulty</WidgetId>
          <Type>GroupButton</Type>
          <Options>size=4</Options>
          <ValueSpace>
            <Value>
              <Key>0</Key>
              <Name>Débutant</Name>
            </Value>
            <Value>
              <Key>1</Key>
              <Name>Avancé</Name>
            </Value>
            <Value>
              <Key>2</Key>
              <Name>Expert</Name>
            </Value>
          </ValueSpace>
        </Widget>
      </Row>
      <Row>
        <Name>Version</Name>
        <Widget>
          <WidgetId>unused</WidgetId>
          <Name>v1.0.0</Name>
          <Type>Text</Type>
          <Options>size=4;fontSize=normal;align=left</Options>
        </Widget>
      </Row>
      <Row>
        <Name>A propos</Name>
        <Widget>
          <WidgetId>widget_1</WidgetId>
          <Name>par @SteveSfartz (developer.cisco.com)</Name>
          <Type>Text</Type>
          <Options>size=4;fontSize=normal</Options>
        </Widget>
      </Row>
      <Options>hideRowNames=0</Options>
    </Page>
    <Name>Jouer</Name>
  </Panel>
</Extensions>